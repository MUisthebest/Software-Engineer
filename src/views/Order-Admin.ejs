<section class="Users">
    <div class="top">
        <i class="fas fa-bars slidebar-toggle"></i>
    </div>
        <div class="Activity">
            <div class="title">
                <i class='bx bxs-user'></i>
                <span class="text">Manage User</span>
            </div>
            <div class="OrderData">
                <div class="box_title">
                    <span class="data-title">ID</span>
                    <span class="data-title">Customer ID</span>
                    <span class="data-title">Total Cost</span>      
                </div>
                <div class="box_data">
                    <% for (let i = 0; i < orders.length; i++){ %>
                    <div class="item">
                        <div class="data ID">
                            <span class="data-list"><%=orders[i].id%></span>
                        </div>
                        <div class="data name">
                            <span class="data-list"><%=orders[i].user%></span>
                        </div>
                        <div class="data address">
                            <span class="data-list"><%=orders[i].totalPrice%></span>
                        </div> 
                    </div>  
                    <% } %>                          
                </div>
            </div>
        </div>             
    </div>
</section>