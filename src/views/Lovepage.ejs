<section class="Wishlist">
    <aside id = "wishlist">
        <h2 class="center">My Wishlist</h2>
    </aside>    
    <div class="gameBx" id="game_bx_2">       
        <% for (let i = 0; i < products.length; i++){ %>
            <div class="product">
                <a href="/products/<%=products[i].id%>" id="<%=products[i].id%>" class="wish">
                    <img src="data:image/<%=products[i].image.contentType%>;base64,<%=products[i].image.data.toString('base64')%>">
                    <div class="content">
                        <div class="left">
                            <h5><%=products[i].name%></h5> 
                            <button class="buy_now"><p class="p1">Buy Now</p><p class="p2"><%=products[i].price%> $</p></button>
                        </div>
                    </div>
                </a>
            </div>
        <% } %>              
    </div>
</section>
<script src="/js/importWishlist.js"></script> 
